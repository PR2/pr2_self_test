<launch>
  <group ns="test_controller" clear_params="true">
    <param name="type" type="string" value="CheckoutController" />
    <param name="timeout" type="double" value="30.0" />
  </group>

  <node pkg="qualification" type="visual_verifier.py" name="wrist_viewer"
        args="$(find qualification)/tests/wrist_test/view.vcg" />

  <node pkg="pr2_controller_manager" type="spawner.py"
        name="wrist_checkout_spawner" args="test_controller" />
  
  <node pkg="qualification" type="robot_checkout.py" name="wrist_checkout" />
</launch>
